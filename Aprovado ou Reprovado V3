import java.util.Scanner;
public class Aprovados_ou_Reprovados_V3 {
	
	public static void main(String[] args) {
			Scanner scanner = new Scanner(System.in);
			
			float a,b,c, media;
			
			a = scanner.nextFloat();
		    b = scanner.nextFloat();

		      media = (a + b) /2;
			System.out.println( media );
		
		if ( media >= 6 ) {
			System.out.println("REPROVOU-SE");
		}	else
				if(media <3) {
		              System.out.println("REPROVOU-SE");
				}else {
	              System.out.println("EXAME");
	              c = scanner.nextFloat();
	              
	              media = ( media + c) / 2;
	              System.out.println(media);
	            		            		  
	            if (media >=5) {
		            System.out.println("APROVADO");
	            } else {
		            	 System.out.println("REPROVOU-SE");
		              }
	            }
				}
	}
